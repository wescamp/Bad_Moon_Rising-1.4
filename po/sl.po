msgstr ""

#. [message]: speaker=Belleros
#: Bad_Moon_Rising/scenarios/ZZ32_abcamp.cfg:142
msgid "This is an old fort..."
msgstr ""

#. [message]: speaker=Mal Behar
#: Bad_Moon_Rising/scenarios/ZZ32_abcamp.cfg:146
